<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>price_greater_or_equal_to_zero</fullName>
    <active>true</active>
    <errorConditionFormula>ROUND(price__c,0)  &lt; 0</errorConditionFormula>
    <errorMessage>Price must be greater then or equal to zero</errorMessage>
</ValidationRule>
